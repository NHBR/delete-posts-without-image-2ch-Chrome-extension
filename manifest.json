{
  "manifest_version": 2,
  "name": "No media post deliter",
  "version": "2.0.0",
  "author" : "NHBR",
  "browser_action": {
		"default_title": "delete posts without image on 2ch",
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
  "content_scripts": [
  {
    "matches": [
      "https://2ch.hk/*",
      "https://boards.4channel.org/*"
    ],
    "js": [ "main.js"]
  }
],
"permissions": ["activeTab"]
}

